{
  "name": "simple-binance-trading-bot-(testnet)",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:client": "vite build",
    "preview": "vite preview",
    "server": "ts-node-dev --respawn --transpile-only server/server.ts",
    "build:server": "tsc -p server/tsconfig.json",

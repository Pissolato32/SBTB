    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^8.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "technicalindicators": "^3.1.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^0.0.48",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@types/ws": "^8.5.10",
    "concurrently": "^8.2.2",
    "ts-node-dev": "^2.0.0",

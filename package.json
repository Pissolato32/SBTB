  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "concurrently": "^8.2.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  }
}